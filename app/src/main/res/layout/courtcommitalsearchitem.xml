                android:layout_height="wrap_content"
                android:text=":"
                android:textColor="#135B97"
                android:layout_weight=".1"/>
            <TextView
                android:id="@+id/policestation"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textColor="#135B97"
                android:layout_weight=".5"
                android:textSize="@dimen/textsize"/>
