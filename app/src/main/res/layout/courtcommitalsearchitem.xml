                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textColor="#135B97"
                android:layout_weight=".2"
                android:text="@string/enter_status"
                android:textSize="@dimen/textsize"/>
            <TextView
                android:id="@+id/commitalstatus1"
